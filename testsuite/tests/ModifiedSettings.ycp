{

    include "testsuite.ycp";

    map READ = $[
	"etc" : $[ "install_inf" : $[] ],
	"target" : $[ "ycp" : $[] ],
	"probe" : $[ "system" : []],
    ];

    TESTSUITE_INIT([READ], nil);
    import "ProfileManager";

    ProfileManager::profile_settings =
    $[ "name": "default",
       "description": "Default description",
       "prestart": "/tmp/default/p1",
       "posttart": "",
       "prestop": "",
       "poststop": "/var/scpm/default/ps"
    ];

    map new_settings =
    $[ "name": "default",
       "description": "Default description",
       "prestart": "/tmp/default/p1",
       "posttart": "",
       "prestop": "",
       "poststop": "/var/scpm/default/ps"
    ];


    DUMP(new_settings);

	TEST (``(ProfileManager::ModifiedSettings(new_settings)), [ $[], $[], $[] ], nil);

    new_settings =
    $[ "name": "default",
       "description": "Default description",
       "prestart": "/tmp/default/p1",
       "posttart": "",
       "prestop": "/tmp/prest",
       "poststop": "/var/scpm/default/ps"
    ];

    DUMP(new_settings);

	TEST (``(ProfileManager::ModifiedSettings(new_settings)), [ $[], $[], $[] ], nil);

}
