{
    // testedfiles: ProfileManager.ycp
    import "Testsuite";
    import "ProfileManager";

    map EX_nil =
    $[ "target":
        $[ "bash": nil ]
    ];

    map EX0 =
    $[ "target":
        $[ "bash": 0 ]
    ];

    map EX1 =
    $[ "target":
        $[ "bash": 1 ]
    ];

    map new_settings =
    $[ "prestart": "/tmp/default/p1",
       "posttart": "",
       "prestop": "",
       "poststop": "/var/scpm/default/ps"
    ];

    Testsuite::Dump (new_settings);

    Testsuite::Test (``(ProfileManager::CheckFilesValidity(new_settings)), [ $[], $[], EX0 ], nil);

    Testsuite::Test (``(ProfileManager::CheckFilesValidity(new_settings)), [ $[], $[], EX1 ], nil);
}
