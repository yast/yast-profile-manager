{

    include "testsuite.ycp";

    map READ = $[
	"etc" : $[ "install_inf" : $[] ],
	"target" : $[ "ycp" : $[] ],
	"probe" : $[ "system" : []],
    ];

    TESTSUITE_INIT([READ], nil);
    import "ProfileManager";

    define TEST_It_Now (string name) ``{

        DUMP("name to check:");
        DUMP(name);
	    TEST (``(ProfileManager::CheckNameValidity(name)), [ $[], $[], $[] ], nil);
    }

    TEST_It_Now ("new name");
    TEST_It_Now ("new_name");
    TEST_It_Now ("_new_name");

}
